        }
    ],
    "require": {
        "symfony/console": "^5.2",
        "kherge/file-manager": "^2.7",
        "symfony/http-foundation": "^5.2",
        "vlucas/spot2": "^2.2",
        "digitalnature/php-ref": "^1.3",
        "minime/annotations": "^3.1",
        "twig/twig": "^3.0",
        "jan-swiecki/simple-annotations": "^0.3.1",
        "doctrine/annotations": "^1.11",
        "haydenpierce/class-finder": "^0.4.2",
        "maximebf/debugbar": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Console\\": "bin/src",
            "App\\Model\\": "model/",
            "App\\Controller\\": "controller/",
            "App\\Helper\\" : "helper/",
            "App\\Config\\" : "config/",
            "App\\": "core/",
            "Entity\\": "entity/",
            "Form\\": "form/"
        },
        "classmap": ["bin/src", "model/", "controller/", "helper/", "config/", "core/", "entity/", "form/"]
    }
}
